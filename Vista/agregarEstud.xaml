<Window x:Class="Deportes_WPF.Vista.agregarEstud"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Deportes_WPF.Vista"
        mc:Ignorable="d"
        Title="agregarEstud" Width="1366" Height="768"
        WindowState="Maximized"
        WindowStyle="SingleBorderWindow" Closed="Window_Closed" Closing="Window_Closing">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1048*"/>
            <ColumnDefinition Width="311*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="lab1" Content="Nombre del Proyecto" HorizontalContentAlignment="Center" HorizontalAlignment="Center" Margin="10,7,10,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="36" Width="1338" Grid.ColumnSpan="2"/>
        <Label x:Name="lab2" Content="Agregar Estudiante al Gimnasio" HorizontalContentAlignment="Center" HorizontalAlignment="Center" Margin="502,112,180,0" VerticalAlignment="Top" RenderTransformOrigin="-0.013,-0.288" FontWeight="Bold" FontSize="24"/>
        <Button x:Name="btn1" Content="Volver" HorizontalAlignment="Right" Margin="202,128,33,0" VerticalAlignment="Top" Width="76" Grid.Column="1" Click="btn1_Click"/>
        <Button x:Name="btn3" Content="Guardar" HorizontalAlignment="Center" Margin="502,679,444,0" VerticalAlignment="Top" Width="101" FontWeight="Bold" FontSize="16" Height="43" Click="btn3_Click">
            <Button.Background>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.MenuHighlightColorKey}}"/>
            </Button.Background>
        </Button>
        <Button x:Name="btn4" Content="Cancelar" HorizontalAlignment="Center" Margin="804,679,141,0" VerticalAlignment="Top" Width="102" FontWeight="Bold" FontSize="16" Height="43" Background="#FFD36D19" Click="btn4_Click"/>
        <Label x:Name="lab3" Content="Nombres" HorizontalAlignment="Center" Margin="495,202,456,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20"/>
        <TextBox x:Name="txt1" HorizontalAlignment="Center" Height="37" HorizontalContentAlignment="Left" Margin="664,206,189,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="194" FontSize="20"/>
        <Label x:Name="lab4" Content="Apellidos" HorizontalAlignment="Center" Margin="495,244,454,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20"/>
        <TextBox x:Name="txt2" HorizontalAlignment="Center" Height="37" HorizontalContentAlignment="Left" Margin="664,248,189,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="194" FontSize="20"/>
        <Label x:Name="lab5" Content="Código" HorizontalAlignment="Center" Margin="495,286,475,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20" RenderTransformOrigin="0.182,0.378"/>
        <TextBox x:Name="txt3" HorizontalAlignment="Center" Height="37" HorizontalContentAlignment="Left" Margin="664,290,189,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="194" FontSize="20"/>
        <Label x:Name="lab6" Content="Carrera" HorizontalAlignment="Center" Margin="495,328,473,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20"/>
        <TextBox x:Name="txt4" HorizontalAlignment="Center" Height="37" HorizontalContentAlignment="Left" Margin="664,332,189,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="194" FontSize="20"/>
        <Label x:Name="lab7" Content="Semestre" HorizontalAlignment="Center" Margin="495,370,456,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20"/>
        <TextBox x:Name="txt5" HorizontalAlignment="Center" Height="37" HorizontalContentAlignment="Left" Margin="664,374,189,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="194" FontSize="20"/>
        <Label x:Name="lab8" Content="Email" HorizontalAlignment="Center" Margin="495,416,491,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20"/>
        <TextBox x:Name="txt6" HorizontalAlignment="Center" Height="37" HorizontalContentAlignment="Left" Margin="664,416,189,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="194" FontSize="20"/>
        <Label x:Name="lab10" Content="Observaciones" HorizontalAlignment="Center" Margin="596,506,284,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="20" Width="167"/>
        <TextBox x:Name="txt7" HorizontalAlignment="Center" Height="117" HorizontalContentAlignment="Left" Margin="502,543,187,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="358" FontSize="20"/>
        <Button x:Name="btn2" Content="Limpiar" HorizontalAlignment="Right" Margin="0,170,33,0" VerticalAlignment="Top" Width="76" Grid.Column="1" Click="btn2_Click"/>

    </Grid>
</Window>
