<Window x:Class="AlphaSport.Vista.Main"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        
        Width="1600" Height="900" Closed="Window_Closed" Closing="Window_Closing"
        WindowState="Maximized" WindowStartupLocation="CenterScreen"
        WindowStyle="None">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/GonzalezPerezCO;component/Recursos/LOGOS/binario.jpg" Stretch="UniformToFill" Opacity="0.685"/>
        </Grid.Background>
        <Image Width="925" HorizontalAlignment="Center"  VerticalAlignment="Top" Source="/GonzalezPerezCO;component/Recursos/LOGOS/azul-gigante.png" Margin="333,48,334,0" Stretch="None"/>
        <Button x:Name="btn2" Content="Deportes" Width="789" Height="892" FontSize="48" FontWeight="Bold" Click="Btn2_Click" Background="{x:Null}" BorderBrush="{x:Null}" Margin="803,0,0,0"/>
        <Button x:Name="btn1" Content="Gimnasio" Width="798" Height="892" FontSize="48" FontWeight="Bold" Click="Btn1_Click" Background="{x:Null}" BorderBrush="{x:Null}" Margin="0,0,794,0"/>
    </Grid>
</Window>
